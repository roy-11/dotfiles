{
  "title": "操作スペースの移動",
  "rules": [
    {
      "description": "操作スペースの移動(fn + y,u,i)",
      "manipulators": [
        {
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "command", "option", "shift"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": ["control"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "command", "option", "shift"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": ["control"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "command", "option", "shift"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow",
              "modifiers": ["control"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "操作スペースの移動(ctrl + y,u,i)",
      "manipulators": [
        {
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["caps_lock", "command", "option", "shift", "fn"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": ["control"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["caps_lock", "command", "option", "shift", "fn"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": ["control"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["caps_lock", "command", "option", "shift", "fn"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow",
              "modifiers": ["control"]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
