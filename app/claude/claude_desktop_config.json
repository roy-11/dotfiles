{
  "mcpServers": {
    "claude_code": {
      "command": "~/.nodenv/shims/claude",
      "args": ["mcp", "serve"],
      "env": {}
    "Framelink Figma MCP": {
      "command": "~/.nodenv/shims/npx",
      "args": [
        "-y",
        "figma-developer-mcp",
        "--stdio"
      ]
    },
    "mcp_server_mysql": {
      "command": "~/.nodenv/shims/npx",
      "args": ["-y", "@benborla29/mcp-server-mysql"],
      "env": {
        "MYSQL_HOST": "127.0.0.1",
        "MYSQL_PORT": "13306",
        "MYSQL_USER": "root",
        "MYSQL_PASS": "",
        "ALLOW_INSERT_OPERATION": "true",
        "ALLOW_UPDATE_OPERATION": "true",
        "ALLOW_DELETE_OPERATION": "true"
      }
    },
    "anki": {
      "args": ["--yes", "anki-mcp-server"]
    },
    "mcp-obsidian": {
      "command": "~/.nodenv/shims/npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "mcp-obsidian",
        "--config",
        "{\"vaultPath\":\"~/Library/Mobile Documents/iCloud~md~obsidian/Documents/Obsidian Vault\"}"
      ]
    },
    "git": {
      "command": "uvx",
      "args": ["mcp-server-git"]
    },
    "claude_code": {
      "command": "~/.nodenv/shims/claude",
      "args": ["mcp", "serve"],
      "env": {}
    },
    "context7": {
      "command": "~/.nodenv/shims/npx",
      "args": ["-y", "@upstash/context7-mcp"]
    }
  },
  "isUsingBuiltInNodeForMcp": false
}
